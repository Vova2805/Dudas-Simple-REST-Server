<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.spring.rest.entities.Rating" table="rating">
        <meta attribute="class-description">
            This class contains the rating detail
        </meta>
        <id name="id" type="java.lang.Integer" column="id">
            <generator class="native"/>
        </id>
        <property name="userId" column="userId" type="java.lang.Integer"/>
        <property name="movieId" column="movieId" type="java.lang.Integer"/>
        <property name="rating" column="rating" type="java.lang.Integer"/>
        <property name="ratingDate" column="ratingDate" type="java.util.Date"/>
    </class>
</hibernate-mapping>